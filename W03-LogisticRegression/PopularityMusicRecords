# 1. UNDERSTANDING THE DATA  

songs = read.csv("songs.csv")
str(songs)

table(songs$year == 2010)

MJ = subset(songs, artistname == "Michael Jackson")
str(MJ) 
nrow(MJ)

MJ$Top10
MJ$songtitle[MJ$Top10 == 1]

table(songs$timesignature)
names(songs)
songs$songtitle[which.max(songs$tempo)]





